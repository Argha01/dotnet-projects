@model List<WeatherDTO>

<div class="text-center">
    <h1 class="display-5" >Welcome to Human Resource Page</h1>
    <table class="table table-bordered table-responsive">
        <thead>
            <tr>
                <th>Date</th>
                <th>Temp - C</th>
                <th>Temp - F</th>
                <th>Summary</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in Model)
            {
                <tr>
                    <td>@item.Date</td>
                    <td>@item.TemperatureC</td>
                    <td>@item.TemperatureF</td>
                    <td>@item.Summary</td>
                </tr>
            }
        </tbody>
    </table>
</div>